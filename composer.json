{
  "name": "eest1/jardin",
  "description": "Proyecto para administrar bibliotecas de jardines",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "bootstrap/app.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "RC",
  "require": {},
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize",
      "npm install"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize",
      "npm update"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "prefer-stable": true,
    "sort-packages": true
  }
}
